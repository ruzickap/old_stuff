<settings>
    <audiocds>
        <autorun>true</autorun>
    </audiocds>
    <dvds>
        <autorun>true</autorun>
    </dvds>
    <input>
        <enablemouse>false</enablemouse>
    </input>
   <locale>
        <country>CZ</country>
        <language>Czech</language>
    </locale>
    <musiclibrary>
        <downloadinfo>true</downloadinfo>
        <updateonstartup>true</updateonstartup>
    </musiclibrary>
    <network>
        <bandwidth>2048</bandwidth>
    </network>
    <powermanagement>
        <shutdowntime>10</shutdowntime>
    </powermanagement>
    <services>
        <esallinterfaces>true</esallinterfaces>
        <webserver>true</webserver>
        <webserverpassword>xbmcclient</webserverpassword>
    </services>
    <slideshow>
        <staytime>2</staytime>
    </slideshow>
    <subtitles>
        <charset>CP1250</charset>
        <color>0</color>
        <height>32</height>
    </subtitles>
    <videolibrary>
        <updateonstartup>true</updateonstartup>
    </videolibrary>
</settings>
